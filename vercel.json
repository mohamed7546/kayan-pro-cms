{
    "version": 2,
    "version": 2,
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/index.py"
        },
        {
            "source": "/calculator",
            "destination": "/public/calculator.html"
        },
        {
            "source": "/admin",
            "destination": "/admin/index.html"
        },
        {
            "source": "/admin/((?!.*\\..*).*)",
            "destination": "/admin/index.html"
        },
        {
            "source": "/((?!api/|admin|calculator|.*\\..*).*)",
            "destination": "/public/index.html"
        },
        {
            "source": "/",
            "destination": "/public/index.html"
        }
    ]
}